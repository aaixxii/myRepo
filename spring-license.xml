<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="classpath*:/jp/co/nec/aim_xm/license/resources/spring/services/spring-floating-license.xml" />

	<bean id="licenseManager"
		class="jp.co.nec.aim.mm.license.LicenseManager">
		<property name="licenseBroker" ref="licenseBroker" />
		<property name="floatingLicenseManager"
			ref="floatingLicenseManager" />
	</bean>
	
</beans>